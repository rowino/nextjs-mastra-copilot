{
  "name": "starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npx concurrently -c \"#93c5fd,#c4b5fd\" \"pnpm run dev:ui\" \"pnpm run dev:agent\" --names=next,mastra --restart-tries 3 --kill-others",
    "dev:agent": "mastra dev",
    "dev:ui": "npx concurrently -c \"#93c5fd,#fde047\" \"wrangler dev\" \"next dev --turbopack\" --names=wrangler,next",
    "dev:debug": "LOG_LEVEL=debug npm run dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@ag-ui/mastra": "0.1.0",
    "@ai-sdk/openai": "^2.0.42",
    "@copilotkit/react-core": "1.10.5",
    "@copilotkit/react-ui": "1.10.5",
    "@copilotkit/runtime": "1.10.5",
    "@daveyplate/better-auth-ui": "^3.2.5",
    "@libsql/client": "^0.15.15",
    "@mastra/client-js": "0.14.1",
    "@mastra/cloudflare-d1": "^0.13.4",
    "@mastra/core": "0.19.1",
    "@mastra/memory": "0.15.4",
    "@opennextjs/cloudflare": "^1.9.1",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@radix-ui/react-slot": "^1.2.3",
    "better-auth": "^1.3.26",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.6",
    "lucide-react": "^0.545.0",
    "mastra": "0.13.4",
    "next": ">=15.5.4",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.26",
    "@cloudflare/workers-types": "^4.20251004.0",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "wrangler": "^4.42.0"
  }
}
